    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="presence" data-view="Hotness" data-title="IT Staff" data-cool="10" data-warm="4"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="bitcoin" data-view="Hotness" data-title="Bitcoins" data-cool="500" data-warm="1000"></div>
    </li>

    
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
